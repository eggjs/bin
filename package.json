    "@types/node": "^18.11.19",
    "assert-file": "^1.0.0",
    "coffee": "^5.4.0",
    "cross-env": "^3.1.3",
    "egg": "^3.9.1",
    "egg-mock": "^5.10.2",
    "esbuild": "^0.17.7",
    "esbuild-register": "^3.4.2",
