    "git-contributor": "2",
    "mm": "^3.2.0",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/egg-bin.git"
  },
  "bug": {
    "url": "https://github.com/eggjs/egg/issues"
  },
  "homepage": "https://github.com/eggjs/egg-bin",
  "author": "fengmk2 <fengmk2@gmail.com> (https://github.com/fengmk2)",
  "scripts": {
    "postinstall": "node bin/postinstall.js",
    "contributor": "git-contributor",
    "lint": "eslint . --cache --ext ts",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "ts-node --swc bin/cli.ts test",
    "test-local-with-ts-node-transpile-only": "node -r ts-node/register/transpile-only bin/cli.ts test",
    "test-local-with-swc": "node -r @swc-node/register bin/cli.ts test",
    "test-local-with-esbuild": "node -r esbuild-register bin/cli.ts test",
    "cov": "c8 -r lcov -r text-summary npm run test-local",
    "ci-test-only": "npm run test-local -- test/lib/cmd/cov.test.js",
    "ci": "npm run lint && npm run test-local && npm run tsc",
    "tsc": "tsc",
    "clean": "tsc -b --clean"
  },
  "engines": {
    "node": ">= 16.19.0"
  }
}
