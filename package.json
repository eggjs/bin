  },
  "dependencies": {
    "@artus-cli/artus-cli": "^0.2.1",
    "@artus-cli/plugin-autocomplete": "^0.1.1",
    "@artus-cli/plugin-version": "^1.0.1",
    "c8": "^7.12.0",
    "detect-port": "^1.3.0",
    "egg-ts-helper": "^1.30.3",
    "egg-utils": "^2.4.1",
    "globby": "^11.1.0",
    "inspector-proxy": "^1.2.2",
    "jest-changed-files": "^29.4.2",
    "mocha": "^10.0.0",
    "mochawesome-with-mocha": "^7.1.3",
    "runscript": "^1.5.3",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "egg-mock": ">=5.10.2"
  },
  "peerDependenciesMeta": {
    "egg-mock": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.3.0",
